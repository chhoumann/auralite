{
  "name": "ovs",
  "version": "0.0.1",
  "description": "",
  "author": "Christian Bager Bach Houmann",
  "type": "commonjs",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "dev": "bun ./esbuild.config.mjs",
    "build": "bun ./esbuild.config.mjs production",
    "lint": "bunx biome lint src build.ts version-bump.ts",
    "format": "bunx biome format src build.ts version-bump.ts --write",
    "check": "bunx @biomejs/biome check --apply src build.ts version-bump.ts && bun run type-check",
    "version": "bun ./version-bump.ts && git add manifest.json versions.json",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bun": "latest",
    "esbuild": "^0.23.0",
    "esbuild-plugin-copy": "^2.1.1",
    "git-contributors": "^0.2.5",
    "obsidian": "^1.5.7-1",
    "semantic-release": "^24.0.0",
    "builtin-modules": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chhoumann/ovs.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}